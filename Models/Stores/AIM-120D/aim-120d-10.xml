<?xml version="1.0"?>
<!-- AIM 120 based on AIM-9L by xii Richard Harrison, 29-06-2015 -->
<PropertyList>
    <path>aim-120d.ac</path>

    <animation>
        <type>select</type>
        <object-name>aim-120d</object-name>
        <condition>
            <property>payload/armament/aim-120d/flags/msl-id-10</property>
        </condition>
    </animation>

    <model>
        <path>Models/Stores/AIM-120D/motor-smoke-10.xml</path>
        <offsets>
            <x-m>3</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </offsets>
    </model>

    <model>
        <path>Models/Stores/AIM-120D/explosion-10.xml</path>
        <offsets>
            <x-m>3</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </offsets>
    </model>

    <animation>
        <object-name>aim-120d</object-name>
        <enable-hot type="bool">false</enable-hot>
    </animation>

</PropertyList>
