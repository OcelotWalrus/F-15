<?xml version="1.0" encoding="iso-8859-1"?>
  
<PropertyList> 

    <path>Aircraft/F-15/Models/Stores/Racks/TER-9A/TER-9A.xml</path>

    <offsets>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.014</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>

    <model>
      <name>Station1_LAU68_left</name>
      <path>Aircraft/F-15/Models/Stores/LAU-68/LAU-68-l-2.xml</path>
      <offsets>
        <x-m>-0.05</x-m>
        <y-m>-0.295</y-m>
        <z-m>-0.08</z-m>
        <pitch-deg>0</pitch-deg>
      </offsets>
    </model>

    <model>
      <name>Station1_LAU68_center</name>
      <path>Aircraft/F-15/Models/Stores/LAU-68/LAU-68-c-2.xml</path>
      <offsets>
        <x-m>-0.05</x-m>
        <y-m>0.115</y-m>
        <z-m>-0.085</z-m>
        <pitch-deg>0</pitch-deg>
      </offsets>
    </model>
    
    <model>
      <name>Station1_LAU68_right</name>
      <path>Aircraft/F-15/Models/Stores/LAU-68/LAU-68-r-2.xml</path>
      <offsets>
        <x-m>-0.05</x-m>
        <y-m>0.10</y-m>
        <z-m>0.325</z-m>
        <pitch-deg>0</pitch-deg>
      </offsets>
    </model>

    <animation>
        <type>rotate</type>
        <object-name>Station1_LAU68_left</object-name>
        <factor>0</factor>
        <offset-deg>-45</offset-deg>
        <center>
          <x-m>-0.1</x-m>
          <y-m>-0.285</y-m>
          <z-m>-0.1</z-m>
        </center>
        <axis>
          <x>1</x>
          <y>0</y>
          <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>Station1_LAU68_center</object-name>
        <factor>0</factor>
        <offset-deg>-45</offset-deg>
        <center>
          <x-m>-0.1</x-m>
          <y-m>-0.285</y-m>
          <z-m>-0.1</z-m>
        </center>
        <axis>
          <x>1</x>
          <y>0</y>
          <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>Station1_LAU68_right</object-name>
        <factor>0</factor>
        <offset-deg>-45</offset-deg>
        <center>
          <x-m>-0.1</x-m>
          <y-m>-0.285</y-m>
          <z-m>-0.1</z-m>
        </center>
        <axis>
          <x>1</x>
          <y>0</y>
          <z>0</z>
        </axis>
    </animation>

    <animation>
      <type>select</type>
      <object-name>Station1_LAU68_left</object-name>
      <condition>
        <equals>
          <property>payload/weight[1]/selected</property>
          <value>LAU-68C</value>
        </equals>
      </condition>
     </animation>

     <animation>
      <type>select</type>
      <object-name>Station1_LAU68_center</object-name>
      <condition>
        <equals>
          <property>payload/weight[1]/selected</property>
          <value>LAU-68C</value>
        </equals>
      </condition>
     </animation>
     
     <animation>
      <type>select</type>
      <object-name>Station1_LAU68_right</object-name>
      <condition>
        <equals>
          <property>payload/weight[1]/selected</property>
          <value>LAU-68C</value>
        </equals>
      </condition>
     </animation>

</PropertyList> 
