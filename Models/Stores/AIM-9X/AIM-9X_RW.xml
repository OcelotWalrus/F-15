<?xml version="1.0"?>

<!-- this is the static non fired model on the aircraft -->

<PropertyList>

	<name>AIM-9X</name>
	<path>AIM-9X.ac</path>


  <animation>
      <type>rotate</type>
      <object-name>aim9x-body</object-name>
      <object-name>aim9x-back-fins</object-name>
      <object-name>aim9x-front-fins</object-name>
      <object-name>aim9x-thrust-vectoring-fins</object-name>
      <object-name>aim9x-mounts</object-name>
      <object-name>aim9x_ir-sphere</object-name>
      <factor>0.0</factor>
      <offset-deg>90</offset-deg>
      <center>
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis>
       <x>1.0</x>
       <y>0.0</y>
       <z>0.0</z>
      </axis>
 </animation>

</PropertyList>
